{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityDb": ""
  },
  "RabbitMq": {
    "HostName": "",
    "UserName": "",
    "Password": "",
    "VirtualHost": "/",
    "Port": 5672,
    "Exchange": "local.notification.events",
    "RoutingKeys": {
      "SmsSend": "notification.sms-send"
    }
  },
  "VerificationCode": {
    "ExpiresIn": "00:05:00"
  },
  "Twilio": {
    "AccountSid": "",
    "AuthToken": "",
    "ServiceSid": ""
  },
  "Jwt": {
    "Key": "",
    "Issuer": "voxpop-identity",
    "Audiences": [
      "voxpop-api"
    ],
    "ExpiresIn": "00:05:00"
  },
  "RefreshToken": {
    "ExpiresIn": "7.00:00:00"
  }
}